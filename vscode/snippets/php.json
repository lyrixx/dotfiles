{
	"Dump Die": {
		"prefix": "dd",
		"body": ["dump($1);die();"],
	},
	"Print memory usage": {
		"prefix": "memory",
		"body": [
			"printf(\"%.2fMb\\n\", memory_get_usage() / 1024 / 1024);"
		]
	},
	"Dump Backtrace": {
		"prefix": "bt",
		"body": [
			"dd(new \\Exception());"
		],
	},
	"Log to file": {
		"prefix": "lf",
		"body": [
			"file_put_contents('log',  __METHOD__ . ': ' . \\$$1 . \"\\n\", FILE_APPEND);"
		],
	}
}
