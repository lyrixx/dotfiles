{
    "workbench.activityBar.visible": true,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.multiCursorModifier": "alt",
    "editor.fontFamily": "consolas,'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.wordWrapColumn": 120,
    "files.exclude": {
        "**/*.pyc": true,
        "**/app/cache/**/doctrine/": true,
        "**/node_modules/": true,
        "**/resources/data/": true,
        "**/var/cache/**/doctrine/": true,
        "**/vendor/**/vendor": true
    },
    "editor.cursorBlinking": "phase",
    "search.useIgnoreFiles": false,
    "workbench.colorTheme": "Monokai",
    "namespaceResolver.leadingSeparator": false,
    "namespaceResolver.sortAlphabetically": true,
    "namespaceResolver.sortNatural": true,
    "files.trimTrailingWhitespace": true,
    "cSpell.userWords": [
        "Blackfire",
        "Cacheable",
        "Debugf",
        "Infof",
        "Metatags",
        "Swarrot",
        "Tracef",
        "aggs",
        "amqp",
        "behat",
        "cobranding",
        "crawlable",
        "errored",
        "goroutines",
        "joli",
        "jsonld",
        "libsodium",
        "loadbalancer",
        "metatag",
        "nyholm",
        "ouibus",
        "pecl",
        "rabbitmq",
        "redirection",
        "redirectionio",
        "segmentations",
        "sendgrid",
        "streadway",
        "symfony",
        "timeouted",
        "userland"
    ],
    "workbench.iconTheme": "vscode-icons",
    "vsicons.dontShowNewVersionMessage": true,
    "go.formatTool": "goimports",
    "go.useLanguageServer": true,
    "files.insertFinalNewline": true,
    "explorer.confirmDelete": false,
    "python.jediEnabled": false,
    "auto-close-tag.SublimeText3Mode": true,
    "auto-close-tag.activationOnLanguage": [
        "*"
    ],
    "emmet.includeLanguages": {
        "twig": "html"
    },
    "cSpell.language": "en,fr",
    "C_Cpp.updateChannel": "Insiders",
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.currentLine.enabled": false,
    "workbench.startupEditor": "newUntitledFile",
    "explorer.confirmDragAndDrop": false,
    "gitlens.codeLens.authors.enabled": false,
    "editor.minimap.showSlider": "always",
    "workbench.editor.enablePreview": false,
    "editor.autoIndent": false,
    "editor.suggestFontSize": 18,
    "window.zoomLevel": 0,
    "editor.fontSize": 22,
    "debug.console.fontSize": 16,
    "terminal.integrated.fontSize": 16,
}
